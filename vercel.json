{ 
    "version": 2,
    "name": "canal olympia",
    "builds": [
        {
            "src": "src/app.ts", 
            "use": "@vercel/node"
        }
    ],
    "routes" : [
        { 
            "src": "/(.*)", 
            "dest": "/app.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "HEAD",
                "OPTIONS",
                "TRACE",
                "CONNECT"
            ]
        }
    ]
}